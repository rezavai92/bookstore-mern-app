div>)\n\n}\n\nexport default Cart","/home/reza/codes/ecommerce-site/client/src/components/client/loader/loader.js",[],"/home/reza/codes/ecommerce-site/client/src/components/client/payment/payment.js",["175","176"],"/home/reza/codes/ecommerce-site/client/src/components/client/navigation/navigation.js",["177","178","179","180","181","182"],"/home/reza/codes/ecommerce-site/client/src/components/client/subjects/subjects.js",["183","184","185","186","187","188","189"],"import React ,{useState,useEffect,useContext} from 'react'\nimport {context} from '../../../contexts/context'\nimport {} from 'react-bootstrap'\nimport Navigation from '../navigation/navigation'\n\nimport { FontAwesomeIcon } from '@fortawesome/react-fontawesome'\nimport { faShoppingCart } from '@fortawesome/free-solid-svg-icons'\n\nconst Subjects = ()=>{\n\n    return(<div>\n\n    </div>)\n}\n\nexport default Subjects\n\n","/home/reza/codes/ecommerce-site/client/src/components/client/authors/authors.js",["190","191","192","193","194"],"import React ,{useState,useEffect,useContext} from 'react'\nimport {context} from '../../../contexts/context'\nimport {ListGroup,Button} from 'react-bootstrap'\nimport Navigation from '../navigation/navigation'\nimport {Link} from 'react-router-dom'\nimport axios from 'axios'\nimport Loader from '../loader/loader'\nimport { FontAwesomeIcon } from '@fortawesome/react-fontawesome'\nimport { faShoppingCart } from '@fortawesome/free-solid-svg-icons'\n\nconst Authors = ()=>{\n\n    const [authors,setAuthors] = useState([])\n    useEffect(()=>{\n\n       async function fetchData (){\n\n                try{\n                  const res= await  axios.get('/api/authors/all');\n\n                  setAuthors(res.data.authors)\n                }\n                catch(error){\n\n                    throw error;\n                }\n        }\n\n        fetchData()\n\n    },[])\n\n    \n\n    const mappedAuthors = authors.map((author)=>{return(<div>\n\n        <ListGroup.Item  >\n       